{
  "name": "rest-api-client-framework",
  "version": "1.0.7",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gawsoftpl/rest-client-framework"
  },
  "homepage": "https://gawsoft.com",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "dev:mock": "mockoon-cli start -d openapi/mockoon.json --pname openapi",
    "dev:mock:stop": "mockoon-cli stop",
    "start:dev": "tsc --watch",
    "start:dev2": "nodemon --watch 'src/**' --ignore 'src/**/*.spec.ts' --exec 'tsc && node example/screenshot.js' ",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generate_model": "docker run --rm -v $PWD/src/models:/local openapitools/openapi-generator-cli generate --global-property models -i /local/api-json.json -g typescript -o /local/"
  },
  "dependencies": {
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@mockoon/cli": "^6.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jimp": "^0.22.10",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
